<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!--
        Adaptive icon canvas: 108dp x 108dp
        Safe zone (always visible): center 72dp x 72dp (66%)
        We size to 68dp to fill most of the safe zone while avoiding clipping
    -->
    <item
        android:width="68dp"
        android:height="68dp"
        android:drawable="@drawable/vitphoe_logo_foreground"
        android:gravity="center" />
</layer-list>
